{
  "name": "portals",
  "version": "1.0.9",
  "description": "Client-side HTTP requests with middleware support.",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push origin master && git push origin master --tags",
