  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HelpfulHuman/Portals/issues"
  },
  "homepage": "https://github.com/HelpfulHuman/Portals",
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.0.1",
    "rollup": "^0.53.0",
    "rollup-plugin-typescript2": "^0.9.0",
    "rollup-watch": "^4.3.1",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {}
